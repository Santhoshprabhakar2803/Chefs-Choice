<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".APP_5"
    android:background="#fff4d7">

    <TextView
        android:id="@+id/tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="55dp"
        android:text="CHEF'S CHOICE"
        android:textSize="30dp"
        android:textStyle="bold"
        android:textColor="#99720c"/>

    <ImageView
        android:id="@+id/iv1"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/mlogo"
        android:contentDescription="A tiny Android icon"/>

    <TextView
        android:id="@+id/tv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CHOOSE YOUR MENU â˜º"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/iv1"
        android:textColor="#735f2a"
        android:textSize="15dp"
        android:textStyle="bold"/>

    <ImageButton
        android:id="@+id/ib1"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/veg"
        android:layout_below="@+id/tv3"
        android:contentDescription="A tiny Android icon"
        android:scaleType="fitCenter"
        android:layout_marginLeft="15dp"
        android:background="#E98D8D"/>

    <ImageButton
        android:id="@+id/ib4"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/nonveg"
        android:layout_below="@+id/tv3"
        android:contentDescription="A tiny Android icon"
        android:scaleType="fitCenter"
        android:layout_marginLeft="210dp"
        android:background="#E98D8D"/>

    <ImageButton
        android:id="@+id/ib2"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/fastfood"
        android:layout_below="@+id/ib1"
        android:contentDescription="A tiny Android icon"
        android:scaleType="fitCenter"
        android:layout_marginLeft="210dp"
        android:background="#E98D8D"/>

    <ImageButton
        android:id="@+id/ib5"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/ice_creamu"
        android:layout_below="@+id/ib1"
        android:contentDescription="A tiny Android icon"
        android:scaleType="fitCenter"
        android:layout_marginLeft="15dp"
        android:background="#E98D8D"/>

    <ImageButton
        android:id="@+id/ib3"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/ship_cake"
        android:layout_below="@+id/ib2"
        android:contentDescription="A tiny Android icon"
        android:scaleType="fitCenter"
        android:layout_marginLeft="15dp"
        android:background="#E98D8D"/>

    <ImageButton
        android:id="@+id/ib6"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/fast_juice"
        android:layout_below="@+id/ib2"
        android:contentDescription="A tiny Android icon"
        android:scaleType="fitCenter"
        android:layout_marginLeft="210dp"
        android:background="#E98D8D"/>



</RelativeLayout>